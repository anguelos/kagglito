<h1>Listing submissions</h1>

<table>
  <tr>
    <th>Responce</th>
    <th>Responcefileext</th>
    <th>Announced</th>
    <th>Submited</th>
    <th>Score</th>
    <th>Chalenge</th>
    <th>Participation</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @submissions.each do |submission| %>
  <tr>
    <td><%= submission.responce %></td>
    <td><%= submission.responcefileext %></td>
    <td><%= submission.announced %></td>
    <td><%= submission.submited %></td>
    <td><%= submission.score %></td>
    <td><%= submission.Chalenge %></td>
    <td><%= submission.participation %></td>
    <td><%= link_to 'Show', submission %></td>
    <td><%= link_to 'Edit', edit_submission_path(submission) %></td>
    <td><%= link_to 'Destroy', submission, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Submission', new_submission_path %>
